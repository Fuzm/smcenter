#指定从那个文件中导入数据
Load.fromFile  = it_mx_pkdkmx.txt

#指定文件中如何分离数据,即分隔符
Load.Separator = |


#指定装载数据模式,1为先导入模式(先删除表,后导入),2为追加导入模式
Load.Mode = 1

#指定要导入到那个表,程序应当检测是否存在该表
#如不存在,应使用该文件配置的建表语句进行建表操作(配置表名时可使用变量)
Load.destTable = it_mx_pkdkmx

#指定建表语句(可使用变量)
Load.createTable = CREATE TABLE IT_MX_PKDKMX (JJBH   VARCHAR(32)      NOT NULL,DKLX   VARCHAR(1)       NOT NULL,JYRQ   VARCHAR(10)      NOT NULL,GYLSH  VARCHAR(20)      NOT NULL,QS     INT              NOT NULL,HKFS   VARCHAR(2)       NOT NULL,BJ     DECIMAL(16,2)    NOT NULL,LX     DECIMAL(16,2)    NOT NULL)
#指定进行加载前需要进行的操作语句(可使用变量),可配置多条,以;号隔开
